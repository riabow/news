<p id="notice"><%= notice %></p>

<h1>Supls</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supls.each do |supl| %>
      <tr>
        <td><%= supl.name %></td>
        <td><%= supl.city %></td>
        <td><%= link_to 'Show', supl %></td>
        <td><%= link_to 'Edit', edit_supl_path(supl) %></td>
        <td><%= link_to 'Destroy', supl, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supl', new_supl_path %>
